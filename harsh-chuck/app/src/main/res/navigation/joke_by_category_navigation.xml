<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/joke_by_category_navigation"
    app:startDestination="@id/categoryJokesFragment">

    <fragment
        android:id="@+id/categoryJokesFragment"
        android:name="com.gmail.harsh_chuck.app.fragments.categoryJokes.CategoryJokesFragment"
        android:label="category_jokes_fragment"
        tools:layout="@layout/category_jokes_fragment" >
        <action
            android:id="@+id/action_categoryJokesFragment_to_jokeFragment"
            app:destination="@id/jokeFragment" />
    </fragment>
    <fragment
        android:id="@+id/jokeFragment"
        android:name="com.gmail.harsh_chuck.app.fragments.joke.JokeFragment"
        android:label="joke_fragment"
        tools:layout="@layout/joke_fragment" />
</navigation>